    "react-element-to-jsx-string": "^14.3.2",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@storybook/addons": "^6.3.9",
    "@storybook/client-api": "^6.3.9",
    "@types/prettier": "^2.4.1",
